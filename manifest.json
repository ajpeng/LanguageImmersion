{
  "description": "Changes random words from one language to another.",
  "manifest_version": 2,
  "name": "Immersion",
  "version": "1.0",

  "icons": {
    "48": "icons/48.png",
    "96": "icons/96.png"
  },

  "content_scripts": [
    {
      "matches": [ "*://*.mozilla.org/*" ],
      "js": [ "immersion.js" ]
    }
  ]

}
